@{ 
    var title = "phần mềm quản lý nhà nghỉ online";
    if (ViewBag.Title != null)
    {
        title = ViewBag.Title;
    }
}
<!DOCTYPE html>
<html lang="en" class="breakpoint-all breakpoint-sm breakpoint-md breakpoint-lg breakpoint-xlg">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="robots" content="index, follow" />
    <!-- Page Title -->
    <title>@Html.Raw(title)</title>

    <!-- Jetpack Open Graph Tags -->
    <meta property="og:type" content="article" />
    <meta property="og:title" content="@Html.Raw(ViewBag.Title)" />
    <meta property="og:url" content="@ViewBag.Url" />
    <meta property="og:description" content="@Html.Raw(ViewBag.Description)" />
    <meta property="article:published_time" content="@ViewBag.CreatedDate" />
    <meta property="article:modified_time" content="@ViewBag.CreatedDate" />
    <meta property="og:site_name" content="Phần mềm quản lý nhà nghỉ online" />
    <meta property="og:image" content="@ViewBag.Image" />
    <meta property="og:image:width" content="1323" />
    <meta property="og:image:height" content="457" />
    <meta property="og:locale" content="vi_VN" />
    <meta name="twitter:site" content="Phần mềm quản lý nhà nghỉ online" />
    <meta name="twitter:text:title" content="@Html.Raw(ViewBag.Title)" />
    <meta name="twitter:image" content="@ViewBag.Image" />
    <meta name="twitter:card" content="summary" />
    <meta property="article:publisher" content="https://www.facebook.com/bizkasa" />
    <meta name="generator" content="bizkasa.com" />
   

    <!-- Meta Tags -->
    <meta charset="utf-8">
    <meta name="keywords" content="@Html.Raw(ViewBag.Keywords)" />
    <meta name="description" content="@Html.Raw(ViewBag.Description)">


    <meta name="viewport" content="width=1190">


    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!--[if IE 8]>
        <link rel="stylesheet" type="text/css" href="css/ie8.css" />
    <![endif]-->
    <!--[if IE 9]>
        <link rel="stylesheet" type="text/css" href="css/ie9.css" />
    <![endif]-->
    <!-- Bootstrap -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/onyx/assets/themify-icons/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/onyx/font-awesome.min.css" type="text/css" rel="stylesheet">
    @*<link href="~/Content/onyx/assets/vegas/vegas.min.css" rel="stylesheet" />*@
    <link href="~/Content/onyx/custom.css" rel="stylesheet" />
    @*<link href="~/Content/fp.css" rel="stylesheet" />*@
    <link href="~/Content/Site.css" rel="stylesheet" />
   
    @*<link rel="bizkasa icon" href="/Content/V2/images/favicon.ico" />*@
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body> 

    <div id="loading" style="display: none;">
        <img src="~/Content/images/loader.gif">
    </div>


    <div ng-app="FontEndModule">


        <!-- Home top header -->
        @Html.Partial("_TopHeader",new Tikasa.MVC.Models.AuthorizeModel())
        <!-- End home top header -->
        @RenderBody()


      
        <!-- jQuery -->
        <footer class="main-footer pt-60 pb-40">
            <div class="container">
               
                <div class="row">
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="footer-copyright pt-40">
                            <p>Bản quyền thuộc về  © Tikasa @DateTime.Now.Year </p>
                        </div>

                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="footer-social-menu pt-40">
                            <ul>
                                <li><a href="https://www.facebook.com/bizkasa" target="_blank" ><span class="ti-facebook"></span></a></li>
                                <li><a href="https://www.youtube.com/channel/UCshNtf05EkYG65dFfuuXYqw" target="_blank"><span class="ti-youtube"></span></a></li>
                                <li><a href="https://plus.google.com/u/0/b/112707970433543066612/"><span class="ti-google"></span></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="sub-footer-menu pt-40">
                            <ul>
                                <li><a href="/Gioi_thieu/Chinh_sach_bao_mat-9">Chính sách bảo mật</a></li>
                                <li><a href="/Gioi_thieu/Cac_dieu_khoan_va_dieu_kien-10">Các điều khoản </a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" ng-controller="UserController">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                       
                        <h5 class="modal-title" id="exampleModalLabel">Đăng nhập</h5>
                        
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <form>
                            <div class="form-group">
                                <div class="alert alert-error" role="alert" ng-show="Message!=undefined">
                                    {{Message}}
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <input type="text" class="form-control form-control-lg" ng-model="User.UserName" placeholder="Email hoặc tên đăng nhập" required>
                                <div class="invalid-feedback">
                                    Bạn cần nhập tên đăng nhập !
                                </div>
                            </div>
                            <div class="form-group">
                                <input type="password" class="form-control form-control-lg" ng-model="User.Password" placeholder="Mật khẩu" required>
                                <div class="invalid-feedback">
                                    Bạn cần nhập tên đăng nhập !
                                </div>
                            </div>
                            <div class="form-group">
                                <button type="submit" class="btn primary btn-block btn-lg" ng-click="Login()">Đăng nhập</button>
                            </div>
                            <div class="form-group">
                           <div class="text-center">
                               <a href="#registerModal" data-toggle="modal" class=" btn-link">Đăng Ký</a>|<a href="" class="  btn-link">Quên mật khẩu ?</a>
                              
                           </div>
                                
                            </div>
                        </form>
                    </div>
                    @*<div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-primary">Send message</button>
                    </div>*@
                </div>
            </div>
        </div>
        <div class="modal fade" id="registerModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" ng-controller="UserController">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Đăng ký tài khoản</h5>
                     
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <form>
                            <div class="form-group">
                                <div class="alert alert-error" role="alert" ng-show="Message!=undefined">
                                  {{Message}}
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <input type="text" ng-model="User.UserName" class="form-control form-control-lg" placeholder="Tên đăng nhập" required>
                                <div class="invalid-feedback">
                                    Bạn cần nhập tên đăng nhập !
                                </div>
                            </div>
                            <div class="form-group">
                                <input type="text" ng-model="User.Email" class="form-control form-control-lg" placeholder="Email" required>
                                <div class="invalid-feedback">
                                    Bạn cần nhập email !
                                </div>
                            </div>
                            <div class="form-group">
                                <input type="password" ng-model="User.Password" class="form-control form-control-lg" placeholder="Mật khẩu" required>
                                <div class="invalid-feedback">
                                    Bạn chưa nhập mật khẩu !
                                </div>
                            </div>
                            <div class="form-group">
                                <button type="submit" ng-click="Register()" class="btn primary btn-block btn-lg">Đăng ký</button>
                            </div>
                            <div class="form-group">
                                <div class="text-center">
                                    <a href="#loginModal" data-toggle="modal" class=" btn-link">Đăng nhập</a>
                                    <small id="passwordHelpBlock" class="form-text text-muted">
                                        Bằng việc nhấn vào nút Đăng ký là bạn đã đồng ý với các điều khoản của chúng tôi.
                                    </small>
                                </div>

                            </div>
                        </form>
                    </div>
                    @*<div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-primary">Send message</button>
                    </div>*@
                </div>
            </div>
        </div>
        
    </div>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Scripts/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- jquery slider image -->

    @*<script src="~/Areas/CPanelAdmin/Content/js/toastr/toastr.js"></script>
    <script src="~/Areas/CPanelAdmin/Content/js/bootstrap.min.js"></script>*@
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/global.js"></script>
    <script src="~/Scripts/app/appModule.js"></script>
    <script src="~/Scripts/app/user.js"></script>
    @RenderSection("Scripts", false)


    <script>

        $(document).on('ready', function () {
            
        });
    </script>

</body>
</html>

