@{
    ViewBag.Title = "Detail";
}

<link href="~/Content/index.css" rel="stylesheet" />
<link href="~/Content/vd.css" rel="stylesheet" />
<div class="ListingBuilder___wrapper" ng-controller="WebsiteController" ng-init="GetWebsite()">
    <input type="hidden" value="@ViewBag.WebsiteId" id="WebsiteId" />
    <section class="section section--ash section--border-top section--border-bottom PageHeaderWrapper___section">
        <div class="PageHeaderWrapper___headerContent">
            <div class="grid grid--align-center grid--bleed grid--justify-space-between">
                <div class="grid__col-sm-8"><h1 class="heading heading--lg">{{Website.Name}}</h1></div>
                <div class="grid__col-sm-3"><button type="button" class="button button--lg button--ivory">Customer Support</button></div>
            </div>
        </div>
    </section>
    <div class="ProgressBar___wrapper">
        <div class="break-point break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
            <div class="grid grid--justify-center">
                <div class="grid__col-xs-12">
                    <div class="progression-bar">
                        <div class="progression-bar__step progression-bar__step--active progression-bar__step--complete">
                            <i class="icon icon--tick progression-bar__step-icon"></i><div class="progression-bar__step-text">
                                <div><p class="text text--bold text--md text--uppercase text--underline">Basic Details</p></div>
                            </div>
                        </div><div class="progression-bar__step progression-bar__step--next">
                            <i class="icon icon--tick progression-bar__step-icon"></i>
                            <div class="progression-bar__step-text"><div><p class="text text--bold text--md text--uppercase">More Details</p></div></div>
                        </div>
                        <div class="progression-bar__step">
                            <i class="icon icon--tick progression-bar__step-icon"></i><div class="progression-bar__step-text">
                                <div><p class="text text--bold text--md text--uppercase">Promote &amp; Pay</p></div>
                            </div>
                        </div>
                        <div class="progression-bar__step">
                            <i class="icon icon--tick progression-bar__step-icon"></i>
                            <div class="progression-bar__step-text"><div><p class="text text--bold text--md text--uppercase">How do you get paid?</p></div></div>
                        </div>
                        <div class="progression-bar__step">
                            <i class="icon icon--tick progression-bar__step-icon"></i><div class="progression-bar__step-text">
                                <div><p class="text text--bold text--md text--uppercase">Verify, Revenue &amp; Expenses</p></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="FormWrapper___formWrapper">
        <div class="FormWrapper___form">
            <div class="grid grid--justify-center">
                <div class="grid" ng-hide="Website.IsCertificated">
                    <div class="grid">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <h2 class="heading heading--lg">Xác minh sở hữu tên miền</h2><hr class="horizontal-rule">
                            </div>
                        </div>
                    </div>

                    <div class="grid" ng-init="Verifycation.IsHtml=true">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <div class="form-group--sm">
                                    <div>
                                        <label class="radio radio--lg" ng-click="ChangeVerifycationMethod()">
                                            <div class="radio__field">
                                                <input type="radio" class="radio__input" name="paymentMethod" value="html" ng-checked="Verifycation.IsHtml" ng-model="Verifycation.Method">
                                                <span class="radio__check"></span>
                                            </div>
                                            <span class="radio__label">Tải file html</span>
                                        </label>
                                        <label class="radio radio--lg" ng-click="ChangeVerifycationMethod()">
                                            <div class="radio__field">
                                                <input type="radio" class="radio__input" name="paymentMethod" value="domain" ng-model="Verifycation.Method">
                                                <span class="radio__check"></span>
                                            </div>
                                            <span class="radio__label">Công cụ quản trị tên miền</span>
                                        </label>
                                        <div data-redux-field-error="paymentMethod"></div>
                                    </div>
                                </div>
                                <hr class="horizontal-rule">
                            </div>
                        </div>
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-5">
                            <div class="break-point break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <p class="HelperText___text">Chúng tôi cần chắc chắn bạn là chủ sở hữu của tên miền hoặc website. Hãy chọn cách xác minh sở hữu tên miền phù hợp với bạn nhất.</p>
                            </div>
                        </div>
                    </div>
                    <div class="grid" ng-show="Verifycation.IsHtml">
                        <div class="grid__col-md-7 grid__col-lg-6 grid__col--bleed-bottom">
                            <div class="grid">
                                <div class="grid__col-auto grid__col--bleed-x grid__col--bleed-top">
                                    <div>
                                        <div class="form-group--sm">
                                            <div>
                                                <label class="input__label">
                                                    <span class="input__label-text">Tải file html lên website</span>

                                                </label>
                                                <div>
                                                    <p>
                                                        <b>1.Tải xuống </b>
                                                        <a href="#" target="_blank">Tệp xác minh này</a>
                                                    </p>
                                                    <p>
                                                        <b>2.Tải lên vào website</b>
                                                    </p>
                                                    <p>
                                                        <b>3.Xác nhận</b>
                                                        tải lên thành công bằng cách truy cập
                                                        <a href="#" target="_blank">link</a> trong trình duyệt của bạn
                                                    </p>
                                                    <p>
                                                        <b>4.Nhấp </b>
                                                        Xác minh dưới đây

                                                    </p>
                                                    <p>
                                                        Để được xác minh, không xóa các tập HTML, ngay cả sau khi xác minh thành công.
                                                    </p>
                                                </div>
                                            </div>
                                            <input type="button" class="btn btn-danger" value="Xác minh" />
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="grid" ng-show="!Verifycation.IsHtml">
                        <div class="grid__col-md-7 grid__col-lg-6 grid__col--bleed-bottom">
                            <div class="grid">
                                <div class="grid__col-auto grid__col--bleed-x grid__col--bleed-top">
                                    <div>
                                        <div class="form-group--sm">
                                            <div>

                                                <label class="input__label">
                                                    <span class="input__label-text">Thực hiện theo các bước bên dưới để tạo bản ghi DNS (Hệ thống tên miền) để chứng minh với Tikasa rằng bạn sở hữu tên miền này.</span>

                                                </label>
                                                <div>
                                                    <p>
                                                        <b>1.Thêm </b>
                                                        bản ghi TXT bên dưới vào cấu hình DNS cho
                                                        <b>abc.com </b>
                                                    </p>
                                                    <p>
                                                        <textarea>tikasa-site-veification:0009ada</textarea>
                                                    </p>
                                                    <p>
                                                        <b>4.Nhấp </b>
                                                        Xác minh dưới đây

                                                    </p>
                                                    <p>
                                                        Khi Chúng tôi tìm thấy bản ghi DNS này, chúng tôi sẽ xác minh bạn là chủ sở hữu miền. (Lưu ý: các thay đổi DNS có thể mất một thời gian. Nếu chúng tôi không tìm thấy bản ghi ngay, chúng tôi sẽ kiểm tra định kỳ).
                                                    </p>
                                                </div>
                                            </div>
                                            <input type="button" class="btn btn-danger" value="Xác minh" />
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <form>
                    <div class="grid">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <h2 class="heading heading--lg">Thông tin website</h2><hr class="horizontal-rule">
                            </div>
                        </div>
                    </div>
                    <div class="grid">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <div class="form-group--md">
                                    <div>
                                        <div class="grid grid--align-start">
                                            <div class="grid__col-sm-6 grid__col--bleed" ng-init="InitMonth()">
                                                <div class="dropdown dropdown--lg">
                                                    <div class="dropdown__label"><span class="dropdown__label-text">Website được public từ khi nào?</span></div>
                                                    <div class="">
                                                        <select class="form-control dropdown dropdown__header" ng-options="row.Key as row.Value for row in Months" ng-model="Website.GoliveMonth">
                                                            <option value=""> Chọn tháng </option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="grid__col-sm-6 grid__col--bleed" ng-init="InitYear()">
                                                <div class="ListingBuilder___inlineElement">
                                                    <div>
                                                        <div class="dropdown dropdown--lg">
                                                            <div class="dropdown__label">
                                                                <span class="dropdown__label-text">&nbsp;</span>
                                                            </div>
                                                            <div>
                                                                <select class="form-control dropdown dropdown__header" ng-options="row.Key as row.Value for row in Years" ng-model="Website.GoliveYear">
                                                                    <option value=""> Chọn năm </option>
                                                                </select>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-5">
                            <div class="break-point break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <p class="HelperText___text">Thời điểm mà website được public cho tất cả người dùng</p>
                            </div>
                        </div>
                    </div>

                    <div class="grid" ng-init="GetTypeOfWebsite()">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <div class="form-group--md">
                                    <div>
                                        <div class="dropdown dropdown--lg">
                                            <div class="dropdown__label"><span class="dropdown__label-text">Website thuộc loại nào?</span></div>
                                            <div>
                                                <select class="form-control dropdown dropdown__header" ng-change="changeTypeOfWebsite()" ng-options="row.Id as row.Name for row in TypeOfWebsites" ng-model="Website.TypeOfWebsiteId">
                                                    <option value=""> Chọn một loại </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div><div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-5">
                            <div class="break-point break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <p class="HelperText___text">Chọn loại danh mục phù hợp với webiste.<br>Ví dụ, nếu website bán hàng, chọn <strong>Content</strong> và chọn <strong>blog</strong> là danh mục con.<br><br></p>
                            </div>
                        </div>
                    </div>
                    <div class="grid" ng-show="SubTypeOfWebsites.length>0">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <div class="form-group--md">
                                    <div class="dropdown dropdown--lg">
                                        <div class="multi-select multi-select--lg multi-select--has-label">
                                            <div class="multi-select__label">
                                                <span class="multi-select__label-text">Danh mục con</span>
                                            </div>
                                            <div>
                                                <select class="form-control dropdown dropdown__header" ng-options="row.Id as row.Name for row in SubTypeOfWebsites" ng-model="Website.SubTypeOfWebsiteId">
                                                    <option value=""> Chọn một loại </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-5">
                            <div class="break-point break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <p class="HelperText___text">Chọn một cụ thể một thể loại phù hợp với nội dung của website.</p>
                            </div>
                        </div>
                    </div>
                    <div class="grid" ng-init="GetCategories()">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <div class="form-group--md">
                                    <div>
                                        <div class="dropdown dropdown--lg">
                                            <div class="dropdown__label">
                                                <span class="dropdown__label-text">Website thuộc lĩnh vực nào?</span>
                                            </div>
                                            <div>
                                                <select class="form-control dropdown dropdown__header" ng-change="changeTypeOfCatefgory()" ng-options="row.Id as row.Name for row in Categories" ng-model="Website.TypeOfCategoryId">
                                                    <option value=""> Chọn một lĩnh vực </option>
                                                </select>
                                            </div>
                                        </div><div data-redux-field-error="niche"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-5">
                            <div class="break-point break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <p class="HelperText___text">Chọn lĩnh vực phù hợp với nội dung của website đăng bán, để người mua có thể dễ dàng tìm kiếm website của bạn</p>
                            </div>
                        </div>
                    </div>

                    <div class="grid" ng-show="SubCategories.length>0">
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                            <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <div class="form-group--md">
                                    <div class="dropdown dropdown--lg">
                                        <div class="multi-select multi-select--lg multi-select--has-label">
                                            <div class="multi-select__label">
                                                <span class="multi-select__label-text">Chọn lĩnh vực con</span>
                                            </div>
                                            <div>
                                                <select class="form-control dropdown dropdown__header" ng-options="row.Id as row.Name for row in SubCategories" ng-model="Website.SubTypeOfCategoryId">
                                                    <option value=""> Chọn lĩnh vực </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-5">
                            <div class="break-point break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                <p class="HelperText___text">Chọn một cụ thể một lĩnh vực để phục vụ cho việc tìm kiếm sau này của người mua</p>
                            </div>
                        </div>
                    </div>

                    @*<div class="grid">
                            <div class="grid__col-lg-6 grid__col--bleed-y grid__col-md-7">
                                <div class="break-point break-point--show-on-base break-point--show-on-small break-point--show-on-medium break-point--show-on-large break-point--show-on-extra-large">
                                    <div class="grid grid--bleed">
