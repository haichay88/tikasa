                var ctx = $("#gachartline");                
                var chart = new Chart(ctx, {
                    type: 'line',
                    data: {
                        labels: labels,
                        
                        datasets: [{
                            label: "User",
                            fill: false,
                            data: usersMonth,
                            backgroundColor: window.chartColors.red,
                            borderColor: window.chartColors.red,
                            borderWidth: 1,

