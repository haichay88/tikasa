                                backgroundColor: window.chartColors.blue,
                                borderColor: window.chartColors.blue,
                                borderWidth: 1
                            }]
                    },

                    options: {
                        scales: {
                            yAxes: [{
                                ticks: {
                                    beginAtZero: true
                                }
                            }]
                        }
                    }
                });


                ///traffic per year
                var usersYear = response[2].result.rows.map(function (row) { return +row[1]; });
                var pageviewsYear = response[2].result.rows.map(function (row) { return +row[2]; });
                var labels = response[2].result.rows.map(function (row) { return +row[0]; });
